<Bindings>
	<Binding name="FELWOODGATHER_TOGGLE_CONFIG_WINDOW" header="FELWOODGATHER" runOnUp="true">
		if ( keystate == "down" ) then
			FelwoodGather_ToggleConfigWindow();
		end
	</Binding>
	<Binding name="FELWOODGATHER_COUNTDOWN" runOnUp="true">
		if ( keystate == "down" ) then
			FelwoodGather_PickupCountDown(true);
		end
	</Binding>
</Bindings>